      <table cellbordercolor="gray" widthtype="fixedleft" rowspacing="0.0785in" columnspacing="0.0785in" refwidth="6.2188in">
        <tablecolumn width="2.3618in"/>
        <tablecolumn width="3.5431in"/>
        <tablerow>
          <tablecell>
            <pardef id="3" keepwithnext="true" keeptogether="true"/>
            <par def="3">
              <run>
                <font size="8pt" style="bold"/>Server name</run>
            </par>
          </tablecell>
          <tablecell>
            <pardef id="4" keepwithnext="true" keeptogether="true"/>
            <par def="4">
              <field type="text" kind="editable" name="serverName"/>
              <compositedata type="98" prevtype="65418" nexttype="222" afterparcount="7" containertype="65418" aftercontainercount="1" afterbegincount="3">
Yg4BAIQAAAAAAAAAAAA=
</compositedata>
            </par>
            <par def="4">
              <run>
                <font size="9pt" color="maroon"/>Entry in format: </run>
              <run>
                <font size="9pt" color="maroon"/>CN=SVEHAPPL01/OU=SRV/O=FIRMA</run>
            </par>
            <par def="4">
              <run>
                <font size="9pt" color="maroon"/>for server: SVEHAPPL01/SRV/FIRMA</run>
            </par>
          </tablecell>
        </tablerow>
        <tablerow>
          <tablecell>
            <par def="3">
              <run>
                <font size="8pt" style="bold"/>ACL exclude</run>
            </par>
            <par def="3">
              <run>
                <font size="8pt" style="bold" color="gray"/>these entries will not be change in Dbs' ACL</run>
            </par>
          </tablecell>
          <tablecell>
            <par def="4">
              <field type="text" allowmultivalues="true" kind="editable" name="aclExclude" listinputseparators="newline" listdisplayseparator="newline"/>
